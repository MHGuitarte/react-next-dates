{
  "version": "0.2.0",
  "name": "react-next-dates",
  "description": "Simple and Customizable DatePicker, DateRangePicker and TimePicker for React.",
  "keywords": [
    "react",
    "typescript",
    "date",
    "date-picker",
    "datepicker",
    "date-range-picker",
    "daterangepicker",
    "date-time-picker",
    "datetimepicker",
    "calendar",
    "clock",
    "date-fns"
  ],
  "author": "Sylvain Castaing @sylcastaing",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sylcastaing/react-next-dates.git"
  },
  "bugs": "https://github.com/sylcastaing/react-next-dates/issues",
  "main": "dist/index.js",
  "module": "dist/react-next-dates.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "test-watch": "tsdx test --watch",
    "coverage": "tsdx test --coverage",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006 --no-manager-cache",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "version": "tsdx build && storybook-to-ghpages",
    "release": "np",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "@popperjs/core": "2.7.1",
    "classnames": "2.2.6",
    "react-popper": "2.2.4"
  },
  "peerDependencies": {
    "date-fns": "^2.0.0",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@babel/core": "7.12.17",
    "@size-limit/preset-small-lib": "4.9.2",
    "@storybook/addon-docs": "6.1.18",
    "@storybook/addon-essentials": "6.1.18",
    "@storybook/addon-info": "5.3.21",
    "@storybook/addon-links": "6.1.18",
    "@storybook/addons": "6.1.18",
    "@storybook/preset-scss": "1.0.3",
    "@storybook/react": "6.1.18",
    "@storybook/storybook-deployer": "2.8.7",
    "@storybook/theming": "6.1.18",
    "@types/classnames": "2.2.11",
    "@types/node": "14.14.30",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "autoprefixer": "9.8.6",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "css-loader": "5.0.2",
    "cssnano": "4.1.10",
    "date-fns": "2.17.0",
    "husky": "5.0.9",
    "node-sass": "5.0.0",
    "np": "7.4.0",
    "pinst": "2.1.4",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-is": "17.0.1",
    "rollup-plugin-copy": "3.4.0",
    "rollup-plugin-postcss": "4.0.0",
    "sass-loader": "10.1.0",
    "size-limit": "4.9.2",
    "style-loader": "2.0.0",
    "tsdx": "0.14.1",
    "tslib": "2.1.0",
    "typescript": "3.9.7"
  },
  "size-limit": [
    {
      "path": "dist/react-next-dates.cjs.production.min.js",
      "limit": "20 KB"
    },
    {
      "path": "dist/react-next-dates.esm.js",
      "limit": "20 KB"
    }
  ],
  "engines": {
    "node": ">=10"
  }
}
