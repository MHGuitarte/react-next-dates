{
  "version": "0.1.0-1",
  "name": "react-next-dates",
  "description": "An another date, date-time and date range picker",
  "author": "Sylvain Castaing @sylcastaing",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sylcastaing/react-next-dates.git"
  },
  "bugs": "https://github.com/sylcastaing/react-next-dates/issues",
  "main": "dist/index.js",
  "module": "dist/react-next-dates.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
